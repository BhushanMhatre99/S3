  body
    .page
      .page-main
        .header.py-4
          .container
            .d-flex
              a.header-brand(href='./home')
                img.header-brand-img(src='img/profile.png', alt='tabler logo')
                | S3
        #headerMenuCollapse.header.collapse.d-lg-flex.p-0
          .container
            .row.align-items-center
              .col-lg.order-lg-first
                ul#myTab.nav.nav-tabs.justify-content-end(role='tablist')
                  li.nav-item
                    a.nav-link.active(href='./register')
                      i.fa.fa-registered
                      |  Register
                  li.nav-item
                    a.nav-link(href='./finger')
                      i.fa.fa-registered
                      |  Finger
                  li.nav-item.dropdown
                    a.nav-link(href='javascript:void(0)', data-toggle='dropdown', aria-expanded='false')
                      i.fe.fe-file
                      |  Report
                    .dropdown-menu.dropdown-menu-arrow(x-placement='bottom-start', style='position: absolute; transform: translate3d(12px, 55px, 0px); top: 0px; left: 0px; will-change: transform;')
                      a.dropdown-item(href='/report')
                        i.fa.fa-sign-in
                        |  Student list
                      a.dropdown-item(href='attendence')
                        i.fa.fa-address-book
                        |  Attendence
                  li.nav-item
                    a.nav-link(href='./login')
                      i.fa.fa-reorder
                      |  Logout
        .my-3.my-md-5
          .container
            .row
              .col-12
                form.card(action='./register_0', method='post')
                  .card-header
                    h3.card-title Student Registration
                  .card-body
                    .row
                      .col-md-12
                        .form-group
                          label.form-label Student Name
                          input.form-control(name='name', placeholder=' Name...', type='text', required='')
                          br
                          label.form-label Email
                          input.form-control(name='email', placeholder='Email...', type='text')
                          br
                          label.form-label Class
                          input.form-control(name='cls', placeholder='Class...', type='text')
                          br
                          label.form-label Roll No
                          input.form-control(name='rno', placeholder='Roll no...', type='text')
                  .card-footer.text-right
                    .d-flex
                      button.btn.btn-primary.ml-auto(type='submit')
                        | Next
                        i.fa.fa-chevron-circle-right(aria-hidden='true')
                script.
                  require(['jquery', 'selectize'], function ($, selectize) {
                  $(document).ready(function () {
                  $('#input-tags').selectize({
                  delimiter: ',',
                  persist: false,
                  create: function (input) {
                  return {
                  value: input,
                  text: input
                  }
                  }
                  });
                  $('#select-beast').selectize({});
                  $('#select-users').selectize({
                  render: {
                  option: function (data, escape) {
                  return '<div>' +
                  '<span class="image"><img src="' + data.image + '" alt=""></span>' +
                  '<span class="title">' + escape(data.text) + '</span>' +
                  '</div>';
                  },
                  item: function (data, escape) {
                  return '<div>' +
                  '<span class="image"><img src="' + data.image + '" alt=""></span>' +
                  escape(data.text) +
                  '</div>';
                  }
                  }
                  });
                  $('#select-countries').selectize({
                  render: {
                  option: function (data, escape) {
                  return '<div>' +
                  '<span class="image"><img src="' + data.image + '" alt=""></span>' +
                  '<span class="title">' + escape(data.text) + '</span>' +
                  '</div>';
                  },
                  item: function (data, escape) {
                  return '<div>' +
                  '<span class="image"><img src="' + data.image + '" alt=""></span>' +
                  escape(data.text) +
                  '</div>';
                  }
                  }
                  });
                  });
                  });
